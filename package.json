  "description": "An Express API for studying and observing cycling data by Ameciclo, focused on promoting and improving cycling infrastructure and safety.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
